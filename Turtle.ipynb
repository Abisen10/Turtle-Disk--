   "source": [
    "from turtle import*\n",
    "import colorsys\n",
    "\n",
    "speed(0)\n",
    "pensize(3)\n",
    "bgcolor('black')\n",
    "hue=0.0\n",
    "\n",
    "for i in range(300):\n",
    "    col=colorsys.hsv_to_rgb(hue,1,1)\n",
    "    pencolor(col)\n",
    "    hue+=0.005\n",
    "    circle(5-i,100)\n",
    "    lt(80)\n",
    "    circle(5-i,100)\n",
    "    rt(100)\n",
    "    done"
   ]
